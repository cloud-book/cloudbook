<lom:lom>
 {{#if metadata.general}}
  <lom:general uniqueElementName="general">
    {{#each metadata.general.identifier}}
    <lom:identifier>
      <lom:catalog uniqueElementName="catalog">{{this.catalog}}</lom:catalog>
      <lom:entry uniqueElementName="entry">{{this.entry}}</lom:entry>
    </lom:identifier>
    {{/each}}
    {{#if metadata.general.title}}
    <lom:title uniqueElementName="title">
        {{#each metadata.general.title}}
      <lom:string language="{{this.titleLang}}">{{this.title}}</lom:string>
        {{/each}}
    </lom:title>
    {{/if}}

     {{#each metadata.general.language}}
    <lom:language>{{this.mainLang}}</lom:language>
    {{/each}}
    {{#each metadata.general.description}}
    <lom:description>
      <lom:string language="{{this.descGeneralLang}}">{{this.Description}}</lom:string>
    </lom:description>
    {{/each}}  
    {{#each metadata.general.keyword}} 
    <lom:keyword>
      <lom:string language="{{this.keywordGeneralLang}}">{{this.keywordGeneral}}</lom:string> 
    </lom:keyword>
    {{/each}}   
    {{#each metadata.general.coverage}} 
    <lom:coverage>
      <lom:string language="{{this.coverageLang}}">{{this.coverage}}</lom:string> 
    </lom:coverage>
    {{/each}} 
    {{#if metadata.general.structure}}
    <lom:structure uniqueElementName="structure">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.general.structure}}</lom:value> 
    </lom:structure>
    {{/if}}
    {{#if metadata.general.aggregationLevel}}
    <lom:aggregationLevel uniqueElementName="aggregationLevel"> 
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.general.aggregationLevel}}</lom:value>
    </lom:aggregationLevel>
    {{/if}}
  </lom:general>      
  {{/if}}
  {{#if metadata.lifeCycle}}
  <lom:lifeCycle uniqueElementName="lifeCycle">
    {{#if metadata.lifeCycle.version}}
    <lom:version uniqueElementName="version">
      {{#each metadata.lifeCycle.version}}
      <lom:string language="{{this.lifeCycleLang}}">{{this.versionlifecycle}}</lom:string>
      {{/each}}
    </lom:version>
     {{/if}}

    {{#if metadata.lifeCycle.status}} 
    <lom:status uniqueElementName="status">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.lifeCycle.status}}</lom:value>
    </lom:status>
    {{/if}}

  {{#each metadata.lifeCycle.contribute}}
    <lom:contribute>
      <lom:role uniqueElementName="role">
        <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
        <lom:value uniqueElementName="value">{{this.rol}}</lom:value>
      </lom:role>
    
      <lom:entity>  
          BEGIN:VCARD VERSION:3.0 FN:{{this.entity.nameContribLifeCycle}}
          EMAIL;TYPE=INTERNET:{{this.entity.emailContribLifeCycle}} ORG:{{this.entity.organContribLifeCycle}}
          END:VCARD
      </lom:entity>
      {{#if this.date.dateContribLifeCycle}}
      <lom:date uniqueElementName="date"> 
         {{#if this.date}}
        <lom:dateTime uniqueElementName="dateTime">{{this.date.dateContribLifeCycle}}</lom:dateTime>
         {{/if}} 
         {{#if this.date.description}}
        <lom:description>
          {{#each this.date.description }}
          <lom:string language="{{this.ContribLifeCycleLang}}">{{this.DescriptionContribLifeCycle}}</lom:string>
            {{/each}} 
        </lom:description>
         {{/if}}
      </lom:date>
      {{/if}}
    </lom:contribute>
   {{/each}} 
     
  </lom:lifeCycle>
  {{/if}}
 
  {{#if metadata.metaMetadata}}
  <lom:metaMetadata uniqueElementName="metaMetadata">
      {{#each metadata.metaMetadata.identifier}}
    <lom:identifier>
      <lom:catalog uniqueElementName="catalog">{{this.metametadataCatalog}}</lom:catalog>
      <lom:entry uniqueElementName="entry">{{this.metametadataEntry}}</lom:entry>
    </lom:identifier>
      {{/each}}
     {{#if metadata.metaMetadata.metadataSchema}} 
    <lom:metadataSchema>{{metadata.metaMetadata.metadataSchema}}</lom:metadataSchema> 
     {{/if}}
      {{#if metadata.metaMetadata.langMetametada}}
    <lom:language>{{metadata.metaMetadata.langMetametada}}</lom:language>
      {{/if}}

      {{#each metadata.metaMetadata.contribute}}
    <lom:contribute>
      
      <lom:role uniqueElementName="role">
        <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
          <lom:value uniqueElementName="value">{{this.rolesMetametadata}}</lom:value>
      </lom:role>
      {{#if this.entity}}
      <lom:entity>  
          BEGIN:VCARD VERSION:3.0 FN:{{this.entity.nameContribMetametadata}}
          EMAIL;TYPE=INTERNET:{{this.entity.emailContribMetametadata}} ORG:{{this.entity.organContribMetametadata}}
          END:VCARD
      </lom:entity>
      {{/if}}
       {{#if this.date.dateContribMetametadata}}     
      <lom:date uniqueElementName="date"> 
        {{#if this.date}}
        <lom:dateTime uniqueElementName="dateTime">{{this.date.dateContribMetametadata}}</lom:dateTime>
        {{/if}}
        {{#if this.date.description}}
        <lom:description>
            {{#each this.date.description }}
          <lom:string language="{{this.ContribMetametadataLang}}">{{this.DescriptionContribMetametadata}}</lom:string>
            {{/each}} 
        </lom:description>
         {{/if}}
      </lom:date>
      {{/if}}
    </lom:contribute>
      {{/each}}    
          
  </lom:metaMetadata>   
  {{/if}} 

{{#if metadata.technical}}   
  <lom:technical uniqueElementName="technical">
    {{#each metadata.technical.format}}
    <lom:format>{{this.formatTechnicalValue}}</lom:format>
    {{/each}}
    
    {{#if metadata.technical.size}}
    <lom:size uniqueElementName="size">{{metadata.technical.size}}</lom:size>
    {{/if}} 
       
    {{#each metadata.technical.location}}
    <lom:location>{{this.locationTechnicalValue}}</lom:location> 
    {{/each}}
      

    {{#each metadata.technical.requirement}} 
    <lom:requirement>
      <lom:orComposite>
        <lom:type uniqueElementName="type">        
          <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
          <lom:value uniqueElementName="value">{{this.typeTechnicalReq}}</lom:value>
        </lom:type>
        
        <lom:name uniqueElementName="name">
          <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
          <lom:value uniqueElementName="value">{{this.nameTechnicalReq}}</lom:value>
        </lom:name>
         {{#if this.minVerTechnicalReq}}
        <lom:minimumVersion uniqueElementName="minimumVersion">{{this.minVerTechnicalReq}}</lom:minimumVersion>
        {{/if}}
        {{#if this.maxVerTecnicalReg}}
        <lom:maximumVersion uniqueElementName="maximumVersion">{{this.maxVerTechnicalReq}}</lom:maximumVersion> 
        {{/if}}  
      
      </lom:orComposite>
    </lom:requirement>  
    {{/each}}

    {{#if metadata.technical.installationRemarks}}      
    <lom:installationRemarks uniqueElementName="installationRemarks"> 
      {{#each metadata.technical.installationRemarks}}
      <lom:string language="{{this.LangRemTech}}">{{this.installRemTechValue}}</lom:string>
      {{/each}}
    </lom:installationRemarks>
    {{/if}}
    
    {{#if metadata.technical.otherPlatformRequirements}}      
    <lom:otherPlatformRequirements> 
      {{#each metadata.technical.otherPlatformRequirements}}
      <lom:string language="{{this.LangOtherTech}}">{{this.requirementsRemTechValue}}</lom:string>
      {{/each}}
    </lom:otherPlatformRequirements>
    {{/if}}

    {{#if metadata.technical.duration}}   
    <lom:duration uniqueElementName="duration">
      <lom:duration uniqueElementName="duration">P{{metadata.technical.duration.durationYearsDurTech}}Y{{metadata.technical.duration.durationMonthsDurTech}}M{{metadata.technical.duration.durationDaysDurTech}}DT{{metadata.technical.duration.durationHoursDurTech}}H{{metadata.technical.duration.durationMinutesDurTech}}M{{metadata.technical.duration.durationSecondsDurTech}}S</lom:duration>
     {{#if metadata.technical.duration.description}}
      <lom:description>
          {{#each metadata.technical.duration.description}}
        <lom:string language="{{this.languageDescDurTech}}">{{this.DescriptionDurTech}}</lom:string>
          {{/each}}
      </lom:description>
      {{/if}}
    </lom:duration>
    {{/if}}  
   
  </lom:technical>
{{/if}}

{{#if metadata.educational}}
  <lom:educational>
 
  {{#if metadata.educational.interactivityType}}  
    <lom:interactivityType uniqueElementName="interactivityType">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.educational.interactivityType}}</lom:value>
    </lom:interactivityType>
  {{/if}}
  
  {{#each metadata.educational.learningResourceType}}
    <lom:learningResourceType>
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{this.resourceTypeEducationalValue}}</lom:value>
    </lom:learningResourceType>
  {{/each}}
  
  {{#if metadata.educational.levelIntEducational}} 
    <lom:interactivityLevel uniqueElementName="interactivityLevel">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.educational.levelIntEducational}}</lom:value>
    </lom:interactivityLevel>
  {{/if}}

  {{#if metadata.educational.levelDensEducational }} 
    <lom:semanticDensity uniqueElementName="semanticDensity">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.educational.levelDensEducational}}</lom:value>
    </lom:semanticDensity>
  {{/if}}

  {{#each metadata.educational.intendedEndUserRole}}
    <lom:intendedEndUserRole>
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{this.endUserEducationalValue}}</lom:value>
    </lom:intendedEndUserRole>
  {{/each}}

  {{#each metadata.educational.context}}
    <lom:context>
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{this.contextEducationalValue}}</lom:value>
    </lom:context>
  {{/each}}

  {{#each metadata.educational.typicalAgeRange}}
    <lom:typicalAgeRange>
      <lom:string language="{{this.languageRangeEducational}}">{{this.rangeAgeEducationalValue}}</lom:string>
    </lom:typicalAgeRange>
  {{/each}}

  {{#if metadata.educational.difficulty}}
    <lom:difficulty uniqueElementName="difficulty">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.educational.difficulty}}</lom:value>
    </lom:difficulty>
  {{/if}}
    {{#if metadata.educational.typicalLearningTime}}
    <lom:typicalLearningTime uniqueElementName="typicalLearningTime">
      <lom:duration uniqueElementName="duration">P{{metadata.educational.typicalLearningTime.durationYearsEducational}}Y{{metadata.educational.typicalLearningTime.durationMonthsEducational}}M{{metadata.educational.typicalLearningTime.durationDaysEducational}}DT{{metadata.educational.typicalLearningTime.durationHoursEducational}}H{{metadata.educational.typicalLearningTime.durationMinutesEducational}}M{{metadata.educational.typicalLearningTime.durationSecondsEducational}}S</lom:duration> 
       {{#if metadata.educational.typicalLearningTime.description}}
      <lom:description>
         {{#each metadata.educational.typicalLearningTime.description}} 
        <lom:string language="{{this.langDurationEducational}}">{{this.DescriptionLearningEducational}}</lom:string>
         {{/each}}
      </lom:description>
      {{/if}}
    </lom:typicalLearningTime>
    {{/if}}
  
  {{#each metadata.educational.description}}
    <lom:description>
      <lom:string language="{{this.langDescrEducational}}">{{this.descEducationUseValue}}</lom:string>
    </lom:description>
  {{/each}}
  
  {{#each metadata.educational.language}}
    <lom:language>{{this.languageEducationalUseValue}}</lom:language>
  {{/each}} 

  {{#each metadata.educational.cognitiveProcess}}
    <lom:cognitiveProcess uniqueElementName="cognitiveProcess">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
        <lom:value uniqueElementName="value">{{this.processcogEducationalValue}}</lom:value>
    </lom:cognitiveProcess>
  {{/each}}

  </lom:educational>           
{{/if}}
 
 {{#if metadata.rights}}
  <lom:rights uniqueElementName="rights">
  
  {{#if metadata.rights.cost}}
    <lom:cost uniqueElementName="cost">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.rights.cost}}</lom:value>
    </lom:cost>
  {{/if}}

  {{#if metadata.rights.copyrightAndOtherRestrictions}}   
    <lom:copyrightAndOtherRestrictions uniqueElementName="copyrightAndOtherRestrictions">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{metadata.rights.copyrightAndOtherRestrictions}}</lom:value>
    </lom:copyrightAndOtherRestrictions>
  {{/if}}

 
   {{#if metadata.rights.description}}
    <lom:description>
      <lom:string language="{{metadata.rights.description.descRightsLang}}">{{metadata.rights.description.descRightsValue}}</lom:string>
    </lom:description>
  {{/if}}
    {{#if metadata.rights.access.accessType}}
    <lom:access uniqueElementName="access">
      <lom:accessType uniqueElementName="accessType">
        <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
        <lom:value uniqueElementName="value">{{metadata.rights.access.accessType}}</lom:value>
      </lom:accessType>
      {{#if metadata.rights.access.description}}
      <lom:description>
        <lom:string language="{{metadata.rights.access.description.descRightsLang}}">{{metadata.rights.access.description.descRightsValue}}</lom:string>
      </lom:description>
      {{/if}}
    </lom:access>
    {{/if}}
  </lom:rights>  
 {{/if}} 
{{#each metadata.relation}}
  <lom:relation>
  
    <lom:kind uniqueElementName="kind">
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{this.kind}}</lom:value>
    </lom:kind>
    {{#if this.resource.catalogRelations}}  
    <lom:resource uniqueElementName="resource">
      <lom:identifier>
        <lom:catalog uniqueElementName="catalog">{{this.resource.catalogRelations}}</lom:catalog>
        <lom:entry uniqueElementName="entry">{{this.resource.entryRelations}}</lom:entry>
      </lom:identifier>
    
    {{#if this.resource.description.DescriptionRelationRelations}}
    <lom:description>
      <lom:string language="{{this.resource.description.relationRelationsLang}}">{{this.resource.description.DescriptionRelationRelations}}</lom:string>
    </lom:description>
    {{/if}}
    </lom:resource>
    {{/if}}
  </lom:relation>
{{/each}}

{{#each metadata.annotation}}
  <lom:annotation>
  {{#if this.entity}}
    <lom:entity>
        BEGIN:VCARD VERSION:3.0 FN:{{this.entity.name}}
        EMAIL;TYPE=INTERNET:{{this.entity.email}} ORG:{{this.entity.organization}} END:VCARD
    </lom:entity>
  {{/if}} 
    {{#if this.date.dateTime}} 
    <lom:date uniqueElementName="date">    
      <lom:dateTime uniqueElementName="dateTime">{{this.date.dateTime}}</lom:dateTime>
       {{#if this.date.description.DescriptionDateAnnotations}}    
      <lom:description>
        <lom:string language="{{this.date.description.langDateAnnotations}}">{{this.date.description.DescriptionDateAnnotations}}</lom:string>
      </lom:description>  
       {{/if}} 
    </lom:date>
    {{/if}} 

  
  {{#if this.description.DescriptionAnnotations}} 
    <lom:description>
      <lom:string language="{{this.description.LangAnnotations}}">{{this.description.DescriptionAnnotations}}</lom:string>
    </lom:description>  
  
  {{/if}} 
  </lom:annotation>
{{/each}}

{{#each metadata.classification}}
  <lom:classification>
    <lom:purpose uniqueElementName="purpose">  
      <lom:source uniqueElementName="source">LOM-ESv1.0</lom:source>
      <lom:value uniqueElementName="value">{{this.purpose}}</lom:value>
    </lom:purpose>


  {{#each this.taxonPath}}  
    <lom:taxonPath>
      <lom:source uniqueElementName="source">
        <lom:string>{{this.sourceNameClassification}}</lom:string>
      </lom:source>  
 
      <lom:taxon>
        <lom:id>{{this.idTaxonClassification}}</lom:id>
          <lom:entry>
            <lom:string language="{{this.langClassificationTaxon}}">{{this.nameTaxonClassification}}</lom:string>
          </lom:entry>
      </lom:taxon>
  
    </lom:taxonPath>
  {{/each}} 
  
  {{#if this.description.DescriptionTaxonClassification}}
    <lom:description>  
      <lom:string language="{{this.description.LangTaxonClassification}}">{{this.description.DescriptionTaxonClassification}}</lom:string>
    </lom:description>
  {{/if}}

  {{#each this.keywords}}  
    <lom:keyword>  
      <lom:string language="{{this.titleLangKeywordTaxonClassification}}">{{this.KeywordTaxonClassification}}</lom:string>
    </lom:keyword>
  {{/each}}   
  </lom:classification>
{{/each}}  

</lom:lom>
      
    
    
