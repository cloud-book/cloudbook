<?xml version="1.0" encoding="UTF-8"?>
	<!--Generated by Cloudbook -->

<manifest identifier="{{objects.ManifestID}}"
     xmlns="http://www.imsglobal.org/xsd/imscp_v1p1"
     xmlns:imsmd="http://www.imsglobal.org/xsd/imsmd_v1p2" 
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xmlns:lom="http://ltsc.ieee.org/xsd/LOM" 
     xsi:schemaLocation = "http://www.imsglobal.org/xsd/imscp_v1p1 imscp_v1p1.xsd http://www.imsglobal.org/xsd/imsmd_v1p2 imsmd_v1p2p2.xsd">

	<metadata>
    	<schema>IMS Content</schema>
    	<schemaversion>1.1.3</schemaversion>
   		{{> imsmetadata}}
	</metadata>


	<organizations default="{{objects.OrganizationID}}">
    	<organization identifier="{{objects.OrganizationID}}"  structure="hierarchical">
      	<title>{{objects.Title_Proyect}}</title>
				{{#each objects.items}}
					{{> imsitem}}
				{{/each}}
       	</organization>
	</organizations>	

	<resources>
		{{#each objects.items}}
			{{> resourceitem}}
		{{/each}}
		{{#each objects.depends}}
		  <resource identifier="{{@key}}" type="webcontent">
    	{{#each this}}
			<file href="{{this}}"/>
		{{/each}}
		  </resource>
    	{{/each}}	 
	</resources>
</manifest>	
