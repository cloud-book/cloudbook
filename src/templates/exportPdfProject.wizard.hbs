<div id="exportPdfProject"class="form-group has-feedback ">
        <div>
		<label for="pagesize">{{gettext "Page size:"}}</label>
			<select class="form-group" name="pagesize"> 
				<option value="A4">A4</option>
				<option value="A3">A3</option>
				<option value="A2">A2</option>
				<option value="Letter">Letter</option>
			</select>
      	</div>
	
	<div>
      		<label for="orientationpage">{{gettext "Page Orientation:"}}</label>
			<label class="radio-inline"><input type="radio" name="orientation" value="Portrait" checked="checked">{{gettext "Portrait"}}</label>
			<label class="radio-inline"><input type="radio" name="orientation" value="Landscape">{{gettext "Landscape"}}</label>
        </div>
     
        
       <div>
      		<label for="headerpage">{{gettext "Include page header:"}}</label>
			<label class="radio-inline"><input type="radio" name="header" value="1">{{gettext "Yes"}}</label>
			<label class="radio-inline"><input type="radio" name="header" value="0" checked="checked">{{gettext "No"}}</label>
                  
                <label for="positionheader"></label>
			<label class="radio-inline"><input type="radio" name="posicionH" value="left" disabled="disabled">{{gettext "Left"}}</label>
			<label class="radio-inline"><input type="radio" name="posicionH" value="center" disabled="disabled" checked="checked">{{gettext "Center"}}</label>
			<label class="radio-inline"><input type="radio" name="posicionH" value="right" disabled="disabled">{{gettext "Right"}}</label>
     </div>	
     
     <div id="headertextcontainer" class="form-group has-feedback">
		<label for="headertext">{{gettext "Text to header page:"}}</label>
		<input type="text" name="headertext" class="form-control" placeholder="{{gettext "Enter header page text"}}" value="{{headertext}}"disabled="disabled" aria-describedby="headertextSuccess2Status"></input>
			<span id="headertextindicator" class="glyphicon form-control-feedback" aria-hidden="true"></span>
			<span id="headertextSuccess2Status" class="sr-only">(success)</span>
     </div>
    
     <div>
      		<label for="numerationpage">{{gettext "Include page numbers in foot page:"}}</label>
			<label class="radio-inline"><input type="radio" name="numeracion" value="1">{{gettext "Yes"}}</label>
			<label class="radio-inline"><input type="radio" name="numeracion" value="0" checked="checked">{{gettext "No"}}</label>
                  
            <label for="positionnumber"></label>
			<label class="radio-inline"><input type="radio" name="posicionN" value="left" disabled="disabled">{{gettext "Left"}}</label>
			<label class="radio-inline"><input type="radio" name="posicionN" value="center" disabled="disabled" checked="checked">{{gettext "Center"}}</label>
			<label class="radio-inline"><input type="radio" name="posicionN" value="right" disabled="disabled">{{gettext "Right"}}</label>
       </div>
    

	<div id="pathcontainer" class="form-group has-feedback">
		<label>{{gettext "Path where save of pdf file:"}}</label><input name="path" type="file" nwsaveas="project.pdf" aria-describedby="pathSuccess2Status"></input>
		<span id="pathindicator" class="glyphicon form-control-feedback" aria-hidden="true"></span>
		<span id="pathSuccess2Status" class="sr-only">(success)</span>

     </div>
</div>

<div> 
<button id="exportpdfback" class="btn" >{{gettext "Cancel"}}</button>
<button id="exportpdfbtn" class="btn" disabled="disabled">{{gettext "Generate pdf"}}</button>
</div>

<div class="waitingOK" style="display:none"><font color="#FC0202"><strong>{{gettext "PDF Generation in progress. Please wait a moment..."}}</strong><div id="pdfCount"></div></font></div>
<div class="waitingFiles" style="display:none"><font color="#FC0202"><strong>{{gettext "PDF Generation in progress. Copying interactive files..."}}</strong></font></div>
<div class="waitingPdf" style="display:none"><font color="#FC0202"><strong>{{gettext "PDF union in progress. Please wait a moment..."}}</strong></font></div>
<div class="waitingER" style="display:none"><font color="#FC0202"><strong>{{gettext "There was an error generating the PDF file. Please cancel de process"}}</strong></font></div>
