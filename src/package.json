{
  "main": "index.html",
  "name": "CloudBook",
  "description": "Graphical editor book",
  "version": "0.2.0",
  "keywords": [
    "editor",
    "book",
    "resources"
  ],
  "single-instance": false,
  "window": {
    "title": "CloudBook",
    "icon": "logo.png",
    "toolbar": false,
    "frame": true,
    "width": 1024,
    "height": 768,
    "position": "mouse"
  },
  "webkit": {
    "plugin": true
  },
  "chromium-args": "--ppapi-flash-path=/usr/lib/adobe-flashplugin/libpepflashplayer.so",
  "dependencies": {
    "fs-extra": "~0.18.0",
    "image-size": "~0.3.5",
    "jed": "~1.1.0",
    "minimist": "~1.2.5",
    "mktemp": "~0.4.0",
    "zip-folder": "^1.0.0",
    "epub-gen": "0.0.9",
    "htmlsplitter" : "0.1.4",
    "epub3tohtml": "0.0.1",
    "pretty-data": "~0.40.0"
  }
}
